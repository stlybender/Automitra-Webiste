<svg width="1200" height="750" xmlns="http://www.w3.org/2000/svg">
  <rect width="1200" height="750" fill="#90D5A4"/>
  <rect x="20" y="20" width="1160" height="710" fill="none" stroke="#000" stroke-width="8"/>
  
  <!-- Halftone pattern -->
  <defs>
    <pattern id="halftone2" patternUnits="userSpaceOnUse" width="10" height="10">
      <circle cx="5" cy="5" r="2" fill="rgba(0,0,0,0.15)"/>
    </pattern>
  </defs>
  <rect width="1200" height="750" fill="url(#halftone2)"/>
  
  <!-- Clock icon -->
  <circle cx="400" cy="300" r="100" fill="#FFD600" stroke="#000" stroke-width="6"/>
  <line x1="400" y1="300" x2="400" y2="220" stroke="#000" stroke-width="8" stroke-linecap="round"/>
  <line x1="400" y1="300" x2="460" y2="300" stroke="#000" stroke-width="8" stroke-linecap="round"/>
  <circle cx="400" cy="300" r="10" fill="#000"/>
  
  <!-- Star burst for perfection -->
  <g transform="translate(800, 300)">
    <polygon points="0,-80 20,-20 80,0 20,20 0,80 -20,20 -80,0 -20,-20" fill="#E89FC4" stroke="#000" stroke-width="6"/>
    <circle cx="0" cy="0" r="30" fill="#fff" stroke="#000" stroke-width="6"/>
    <text x="0" y="10" font-family="Arial, sans-serif" font-size="32" font-weight="900" fill="#000" text-anchor="middle"></text>
  </g>
  
  <!-- Arrow between them -->
  <line x1="520" y1="300" x2="680" y2="300" stroke="#000" stroke-width="6"/>
  <polygon points="680,300 660,285 660,315" fill="#000"/>
  
  <!-- Text -->
  <text x="600" y="580" font-family="Arial, sans-serif" font-size="64" font-weight="900" fill="#000" text-anchor="middle">
    TIMING &gt; PERFECTION
  </text>
  <rect x="250" y="530" width="700" height="80" fill="none" stroke="#000" stroke-width="6"/>
</svg>